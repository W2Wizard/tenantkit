{
  "name": "campaigner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "landlord:seed": "bun --bun run ./scripts/seed.ts",
    "tenant:seed": "bun --bun run ./scripts/tenant-seed.ts",
    "make:tenant": "bun --bun run ./scripts/make-tenant.ts",
    "tenants:list": "bun --bun run ./scripts/tenants-list.ts",
    "tenants:migrate": "bun --bun run ./scripts/tenants-migrate.ts",
    "tenant:migration": "bunx drizzle-kit generate --config=./drizzle/config/tenant.config.ts",
    "landlord:push": "bunx drizzle-kit push --config=./drizzle/config/landlord.config.ts",
    "landlord:migrate": "bun --bun run ./scripts/landlord-migrate.ts",
    "landlord:migration": "bunx drizzle-kit generate --config=./drizzle/config/landlord.config.ts",
    "make:script": "bun --bun run ./scripts/make-script.ts",
    "drizzle:studio": "bun --bun run ./scripts/drizzle-studio.ts",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:studio": "bun --bun run drizzle-kit studio --port 3000 --host 0.0.0.0 --verbose"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@faker-js/faker": "^8.4.1",
    "@inquirer/prompts": "^6.0.1",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.6.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.7",
    "@types/bun": "^1.1.10",
    "@types/eslint": "^9.6.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.44.1",
    "globals": "^15.9.0",
    "openapi-typescript": "^7.4.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "svelte": "^5.0.0-next.260",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  },
  "@clack/prompts": "^0.7.0",
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.5",
    "@isaacs/ttlcache": "^1.4.1",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "bits-ui": "^0.21.15",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "jsonwebtoken": "^9.0.2",
    "lucia": "^3.2.0",
    "lucide-svelte": "^0.446.0",
    "mode-watcher": "^0.4.1",
    "openapi-fetch": "^0.12.2",
    "oslo": "^1.2.1",
    "paneforge": "^0.0.6",
    "postgres": "^3.4.4",
    "qrcode": "^1.5.4",
    "resend": "^3.5.0",
    "svelte-headless-table": "^0.18.2",
    "svelte-sonner": "^0.3.28",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1"
  },
  "trustedDependencies": [
    "svelte-preprocess"
  ]
}
