{
	"name": "campaigner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"landlord:seed": "bun --bun run ./scripts/seed.ts",
		"tenant:seed": "bun --bun run ./scripts/tenant-seed.ts",
		"make:tenant": "bun --bun run ./scripts/make-tenant.ts",
		"tenants:list": "bun --bun run ./scripts/tenants-list.ts",
		"tenants:migrate": "bun --bun run ./scripts/tenants-migrate.ts",
		"tenant:migration": "bunx drizzle-kit generate --config=./drizzle/config/tenant.config.ts",
		"landlord:push": "bunx drizzle-kit push --config=./drizzle/config/landlord.config.ts",
		"landlord:migrate": "bun --bun run ./scripts/landlord-migrate.ts",
		"landlord:migration": "bunx drizzle-kit generate --config=./drizzle/config/landlord.config.ts",
		"make:script": "bun --bun run ./scripts/make-script.ts",
		"drizzle:studio": "bun --bun run ./scripts/drizzle-studio.ts",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"db:studio": "bun --bun run drizzle-kit studio --port 3000 --host 0.0.0.0 --verbose"
	},
	"devDependencies": {
		"prettier": "^3.3.3",
		"@faker-js/faker": "^8.4.1",
		"@inquirer/prompts": "^6.0.1",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/kit": "^2.7.1",
		"prettier-plugin-svelte": "^3.2.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.8",
		"@types/bun": "^1.1.11",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/qrcode": "^1.5.5",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.26.2",
		"globals": "^15.11.0",
		"openapi-typescript": "^7.4.1",
		"svelte": "^5.0.0-next.272",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.10.0",
		"vite": "^5.4.9",
		"vite-plugin-static-copy": "^1.0.6"
	},
	"@clack/prompts": "^0.7.0",
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.6",
		"@isaacs/ttlcache": "^1.4.1",
		"@lucia-auth/adapter-drizzle": "^1.1.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@redis/client": "^1.6.0",
		"@sveltejs/enhanced-img": "^0.3.9",
		"bits-ui": "^0.21.16",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.35.2",
		"jsonwebtoken": "^9.0.2",
		"lucide-svelte": "^0.446.0",
		"mode-watcher": "^0.4.1",
		"openapi-fetch": "^0.12.2",
		"oslo": "^1.2.1",
		"paneforge": "^0.0.6",
		"postgres": "^3.4.4",
		"resend": "^3.5.0",
		"rollup-plugin-copy": "^3.5.0",
		"svelte-headless-table": "^0.18.2",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"uqr": "^0.1.2",
		"zod": "^3.23.8"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@sveltejs/kit",
		"esbuild",
		"sharp",
		"svelte-preprocess"
	]
}
